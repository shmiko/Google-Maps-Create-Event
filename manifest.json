{
  "name": "Google Maps - Add Event",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Allows for creation of calendar events from infowindow in google maps",
  "content_scripts": 
  [
    {
      "matches": ["https://*.google.com/maps/*", "http://*.google.com/maps/*", "http://maps.google.com/*", "https://maps.google.com/*"],
      "js": ["extension.js"],
      "run_at": "document_end"
    }
  ]
}
